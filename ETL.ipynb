   "source": [
    "calculamos el rendimiento anualizado de las empresas utilizando los rendimientos diarios obtenidos y se expresa como un porcentaje."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calcular los retornos diarios de todas las empresas\n",
    "daily_returns = sp500_prices.pct_change()\n",
    "\n",
    "# Calcular el retorno anualizado de cada empresa\n",
    "annual_returns = ((1 + daily_returns.mean()) ** 252) - 1\n",
    "\n",
    "# Obtener información adicional sobre cada empresa\n",
    "sp500_info = table[['Symbol', 'Security', 'GICS Sector', 'GICS Sub-Industry' ]]\n",
    "sp500_info.columns = ['Empresa', 'Nombre', 'Sector', 'Industria' ]"
   ]
  },
